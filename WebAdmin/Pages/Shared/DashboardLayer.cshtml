            -ms-overflow-style: none;
            scrollbar-width: none;
        }

        /* Footer styles */
        .footer {
            background-color: #3c3f41;
            text-align: center;
            width: 100%;
            height: 1vh;
            position: fixed;
            bottom: 0;
            left: 0;
            z-index: 3;
