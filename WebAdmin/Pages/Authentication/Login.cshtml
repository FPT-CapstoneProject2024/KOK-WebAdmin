@page
@model WebAdmin.Pages.Authentication.LoginModel
@{
    ViewData["Title"] = "Login";
    Layout = null;
}


<link rel="stylesheet" href=" \css/login.css">


<div id="login">
    <form method="post">
        <h1>Login</h1>
            <input asp-for="Credential.Email" placeholder="Email" type="text"/>
            <input asp-for="Credential.Password" type="password" placeholder="Password" type="password"/>
        <button type="submit">Log in</button>
    </form>
    <p style="margin-top:30px; color: red;">@ViewBag.Message</p>
</div> 
